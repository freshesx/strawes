{
  "name": "strawes",
  "version": "0.1.3",
  "description": "Dashboard UI extends vue-human by FreshES",
  "author": "ChaselWu <chasel.wu@vitualreaty.com>",
  "license": "MIT",
  "scripts": {
    "start": "node ./scripts/dev.js",
    "dev": "node ./scripts/dev.js",
    "prod": "node ./scripts/prod.js",
    "build": "node ./scripts/prod.js",
    "lint": "eslint --ext .js,.vue src",
    "config": "cp src/env/config.js src/env/config.local.js",
    "git": "git init && git add . && git commit -m 'init project'",
    "first": "yarn install && yarn run config && yarn run git",
    "test": "echo \"No test programs\" && exit 0"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "vue": "^2.3.4",
    "vue-human": "^2.6.11",
    "vue-human-env": "^2.0.2",
    "vue-router": "^2.7.0",
    "vuex": "^2.3.1"
  },
  "devDependencies": {
    "freshes-man": "^0.3.0"
  },
  "engines": {
    "node": ">= 8.0.0",
    "npm": ">= 5.0.0"
  },
  "browserslist": [
    "last 2 versions",
    "iOS >= 7.1",
    "Android >= 4.3",
    "Samsung >= 4",
    "not ie <= 10",
    "not Opera <= 200"
  ]
}
